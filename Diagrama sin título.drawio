        <mxCell id="wzusWgHNVMKydE2oBifo-4" value="&lt;font style=&quot;font-size: 28px;&quot;&gt;Instace Types&lt;/font&gt;" style="ellipse;whiteSpace=wrap;html=1;" vertex="1" parent="1">
